  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kpu.cubism.MemoryMapper">

	<!-- Max 시간 초 -->
	<select id="getMemoryList" parameterType="MemoryVo" resultType="MemoryVo">
  		SELECT
  		 MemoryIdx,
  		 Memoryflag,
  		 MemoryRank,
  		 MemorySec,
  		 MemorymaxSec,
  		 MemoryminSec,
  		 MemoryDate
  		FROM 
  		 memory
  		WHERE
  		 Memoryflag = 'OR'	
	</select>
	
